<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminMenuController">
   <content>
      <BorderPane minHeight="-Infinity" minWidth="-Infinity">
         <center>
            <TabPane tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
              <tabs>
                <Tab fx:id="tabOfManagement" text="Management">
                  <content>
                    <AnchorPane />
                  </content>
                </Tab>
                <Tab fx:id="tabOfAllocations" text="Allocations">
                  <content>
                    <AnchorPane />
                  </content>
                </Tab>
                  <Tab fx:id="tabOfExecutionsHistory" text="Executions history">
                    <content>
                      <AnchorPane />
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </center>
      </BorderPane>
   </content>
</ScrollPane>
